<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <appSettings>
        <add key="TenantId" value="00000000-XXXX-0000000" />
        <add key="AccessKey" value="YourStorageAccountAccessKey" />
        <add key="ManifestURL" value="https://youradls.blob.core.windows.net/dynamics365-financeandoperations/yourenvvi.sandbox.operations.dynamics.com/Tables/Tables.manifest.cdm.json" />
        <add key="TranslateEnum" value ="true" />

        <!--True: Output to console of sql statements without executing. False: executing sql statement in Snowflake. -->
        <add key="TargetSnowflakeDryRun" value ="false" />
        <!-- Process specific tables specified below. Comment out to process for all tables avaiable in the lake. -->
        <add key="tablenames" value ="AssetBook" />

        <!-- Connection string for the target Snowflake account. -->
        <add key="TargetSnowflakeDbConnectionString" value ="account=XXXX.australia-east.azure;user=XXXX;password=XXXX;db=XXXX;schema=XXXX"/>
        <!-- instruct the program to setup Snowflake DB. setup includes:
            1. Create target sechma.
            2. Create file format for the Data Lake in the target schema.
            3. Create external stage for the Data Lake in the target schema.
            4. Create SP_UPDATE_METADATA_DATA_LAKE_FILE_LIST in the target schema.
            5. Create SP_COPY_MAIN in the target schema.
            6. Create TK_COPY_MAIN and TK_COPY_MAIN_FULL_RELOAD  in the target schema. -->
        <add key="TargetSnowflakeSetupDb" value ="false"/>
        <!-- The target Snowflake schema which database objects will be created in. -->
        <add key="TargetSnowflakeDbSchema" value ="XXXX" />
        <!-- Snowflake warehouse used by the created tasks for copy data. -->
        <add key="TargetSnowflakeWarehouse" value ="XXXX" />
        <!-- The Snowflake integation name for the Azure. -->
        <add key="TargetSnowflakeExistingStorageIntegrationName" value="XXXX"/>
    </appSettings>
</configuration>